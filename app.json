{
  "expo": {
    "name": "dwelling",
    "slug": "Loadway-mobile",
    "version": "4.1.0",
    "orientation": "portrait",
    "entryPoint": "./src/App.tsx",
    "icon": "./src/assets/foregroundimages.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./src/assets/adaptive-icon.png"
    },
    "splash": {
      "image": "./src/assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#410019"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.mydwelling",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.schleppproperty.dwelling",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/foregroundimages.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "37775479-68c7-4825-8ff1-0d646d194630"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/37775479-68c7-4825-8ff1-0d646d194630"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/adaptive-icon.png"
        }
      ],
      "expo-build-properties",
      "expo-secure-store"
    ]
  }
}
